<div class="background">	
	<div class="container text-center" id="<%= controller_name %>">
			<div class="title">
				<h1>數位帳號</h1>
			</div>
			<%= form_for @digital_asset, url: will_digital_asset_path(@will, @digital_asset), method: :patch, html: {class: "text-center"} do |f| %>
				<%= render :partial => 'digital_asset', :locals => {:f => f}  %>

				<div class="row">
					<div class="col-md-3"></div>
					<div class="col-md-6">
						<%= f.submit '儲存', class: "submit" %>
					</div>
				</div>
			<% end %>
		</div>
	</div>
</div>
